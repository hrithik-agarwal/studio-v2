# Vite Environment Variables Example
# Copy this file to .env or .env.development and fill in your values
# All client-side variables must be prefixed with VITE_

# APP CONFIG
VITE_SKYPOINT_AI_URL=
VITE_API_BASE_URL=
VITE_SERVICES_EXTERNAL_URL=
VITE_APPLICATION_INSIGHTS_INSTRUMENTATION_KEY=

# B2C Config
VITE_B2C_CONFIG_INSTANCE=
VITE_B2C_CONFIG_TENANT=
VITE_B2C_CONFIG_ISSUER=
VITE_B2C_CONFIG_POLICIES=
VITE_B2C_CONFIG_CLIENT_ID=
VITE_B2C_CONFIG_SCOPES=
VITE_B2C_CONFIG_REDIRECT_URL=
VITE_B2C_CONFIG_CACHE_LOCATION=

# LOGGING CONFIG
# Enable logging in production (automatically enabled in dev)
# VITE_ENABLE_LOGGING=true

# Set log level: debug, info, warn, error (default: debug)
# VITE_LOG_LEVEL=debug
